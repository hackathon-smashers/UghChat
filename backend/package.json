{
  "name": "backend",
  "version": "1.2.0",
  "description": "da backend",
  "main": "index.js",
  "scripts": {
    "gen-env": "gen-env-types .env -o src/env.d.ts -e .",
    "clean": "rimraf -rf build && rimraf ./src/env.d.ts",
    "build:ts": "tsc",
    "build": "npm run build:ts",
    "watch": "concurrently -k -p \"{name}\" -n \",\" -c \"cyan.bold, green.bold\" \"npm run watch:ts\" \"npm run watch:serve\"",
    "watch:ts": "tsc -w",
    "watch:serve": "nodemon -r dotenv-safe/config build/index.js",
    "start": "node build/index.js"
  },
  "keywords": [],
  "license": "MIT",
  "devDependencies": {
    "@types/body-parser": "^1.19.2",
    "@types/cors": "^2.8.12",
    "@types/express": "^4.17.13",
    "@types/node": "^17.0.16",
    "concurrently": "^7.0.0",
    "dotenv-safe": "^8.2.0",
    "gen-env-types": "^1.3.2",
    "nodemon": "^2.0.15",
    "rimraf": "^3.0.2",
    "typescript": "^4.5.5"
  },
  "dependencies": {
    "axios": "^0.26.1",
    "body-parser": "^1.20.0",
    "chalk": "^4.1.1",
    "cors": "^2.8.5",
    "dahvidclient": "^1.2.5",
    "discord.js": "^13.6.0",
    "express": "^4.17.3",
    "firebase": "^9.6.10",
    "moment": "^2.29.1"
  }
}
